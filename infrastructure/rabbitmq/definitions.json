{
  "rabbit_version": "3.13.7",
  "rabbitmq_version": "3.13.7",
  "product_name": "RabbitMQ",
  "product_version": "3.13.7",
  "users": [
    {
      "name": "obs_user",
      "password": "obs_password",
      "tags": ["administrator"]
    },
    {
      "name": "guest",
      "password": "guest",
      "tags": ["administrator"]
    }
  ],
  "vhosts": [
    {
      "name": "observability"
    },
    {
      "name": "/"
    }
  ],
  "permissions": [
    {
      "user": "obs_user",
      "vhost": "observability",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    },
    {
      "user": "obs_user",
      "vhost": "/",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    },
    {
      "user": "guest",
      "vhost": "observability",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    },
    {
      "user": "guest",
      "vhost": "/",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    }
  ],
  "topic_permissions": [],
  "parameters": [],
  "global_parameters": [
    {
      "name": "cluster_name",
      "value": "observability_cluster"
    }
  ],
  "policies": [
    {
      "vhost": "observability",
      "name": "logs-ha-policy",
      "pattern": "^logs\\.",
      "apply-to": "queues",
      "definition": {
        "ha-mode": "all",
        "ha-sync-mode": "automatic",
        "message-ttl": 86400000,
        "max-length": 100000
      },
      "priority": 10
    },
    {
      "vhost": "observability",
      "name": "metrics-ha-policy",
      "pattern": "^metrics\\.",
      "apply-to": "queues",
      "definition": {
        "ha-mode": "all",
        "ha-sync-mode": "automatic",
        "message-ttl": 3600000,
        "max-length": 50000
      },
      "priority": 10
    },
    {
      "vhost": "observability",
      "name": "traces-ha-policy",
      "pattern": "^traces\\.",
      "apply-to": "queues",
      "definition": {
        "ha-mode": "all",
        "ha-sync-mode": "automatic",
        "message-ttl": 86400000,
        "max-length": 50000
      },
      "priority": 10
    }
  ],
  "exchanges": [
    {
      "name": "logs.topic",
      "vhost": "observability",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "metrics.topic",
      "vhost": "observability",
