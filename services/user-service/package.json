{
  "name": "@observability-hub/user-service",
  "version": "1.0.0",
  "description": "User management microservice with observability logging",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && npm run copy-proto",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "send-test-message": "tsx send-test-message.ts",
    "copy-proto": "mkdir -p dist/proto && cp -r ../../proto/ dist/proto/ 2>/dev/null || echo 'No proto files to copy'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "test:unit": "jest --testPathPattern=unit",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "clean": "rm -rf dist",
    "docker:build": "docker build -t log-producer .",
    "docker:run": "docker run -p 3000:3000 -p 50051:50051 log-producer",
    "metrics": "curl http://localhost:3000/metrics",
    "health": "curl http://localhost:3000/health"
  },
  "keywords": [
    "observability",
    "logging",
    "microservice",
    "typescript",
    "rabbitmq",
    "express"
  ],
  "author": "Observability Hub Team",
  "license": "MIT",
  "dependencies": {
    "@observability-hub/observability": "file:observability-hub-observability-1.0.0.tgz",
    "amqplib": "^0.10.3",
    "async-retry": "^1.3.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-slow-down": "^2.0.1",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "module-alias": "^2.2.2",
    "node-config": "^0.0.2",
    "opossum": "^8.0.0",
    "pg": "^8.11.3",
    "prom-client": "^15.1.0",
    "rate-limiter-flexible": "^4.0.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.4",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/ioredis": "^5.0.0",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.4",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3",
    "testcontainers": "^10.2.2",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.2",
    "typescript": "^5.3.2"
  },
  "files": [
    "dist",
    "proto",
    "README.md"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
