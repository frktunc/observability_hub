{
  "name": "@observability-hub/observability",
  "version": "1.0.0",
  "description": "Complete observability solution for microservices - logging, monitoring, health checks, Redis, configuration",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "clean": "rm -rf dist",
    "pack": "npm run build && npm pack"
  },
  "keywords": [
    "observability",
    "logging",
    "monitoring",
    "microservice",
    "typescript",
    "rabbitmq",
    "redis",
    "health-check",
    "circuit-breaker",
    "rate-limiting",
    "metrics",
    "prometheus",
    "configuration"
  ],
  "author": "Observability Hub Team",
  "license": "MIT",
  "dependencies": {
    "amqplib": "^0.10.3",
    "async-retry": "^1.3.3",
    "express": "^5.1.0",
    "ioredis": "^5.3.2",
    "opossum": "^8.0.0",
    "prom-client": "^15.1.0",
    "rate-limiter-flexible": "^4.0.1",
    "uuid": "^11.1.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.4",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.2"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./config": {
      "import": "./dist/config/index.js",
      "require": "./dist/config/index.js",
      "types": "./dist/config/index.d.ts"
    },
    "./redis": {
      "import": "./dist/redis/index.js",
      "require": "./dist/redis/index.js",
      "types": "./dist/redis/index.d.ts"
    },
    "./health": {
      "import": "./dist/health/index.js",
      "require": "./dist/health/index.js",
      "types": "./dist/health/index.d.ts"
    },
    "./middleware": {
      "import": "./dist/middleware/index.js",
      "require": "./dist/middleware/index.js",
      "types": "./dist/middleware/index.d.ts"
    },
    "./dist/middleware": {
      "import": "./dist/middleware/index.js",
      "require": "./dist/middleware/index.js",
      "types": "./dist/middleware/index.d.ts"
    }
  }
}
